<div id="posts">
    <h2>Post Feed</h2>
    <% posts.forEach(post => { %>
        <div class="post" id="post-<%= post.id %>">
            <a href="<%= post.link %>"><h2><%= post.title %></h2></a>
            <h4>Author: <%= post.creator %></h4>
            <h4>Published: <%= post.pubDate%></h4>
            <h4>Categories: <%= post.categories.join(',') %></h4>
            <p><%- post.content %></p>
        </div>   
    <% }) %>
</div>